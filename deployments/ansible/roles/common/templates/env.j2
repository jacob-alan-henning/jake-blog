BLOG_SERVER_PORT=8080
BLOG_REPO_URL=git@github.com:jacob-alan-henning/jake-blob-markdown.git
BLOG_REPO_PRIV_KEY="{{ blog_repo_priv_key }}"
BLOG_REPO_PRIV_KEY_PATH=/tmp/id
BLOG_REPO_PUB_KEY=/tmp/id.pub
BLOG_REPO_PASS={{ blog_repo_pass }}
SSH_KNOWN_HOSTS=/etc/known_hosts
BLOG_CONTENT_DIR=/tmp/content
OTEL_LOG_LEVEL=debug
BLOG_HTTPS_ON=true
BLOG_HTTPSCRT="/etc/letsencrypt/live/{{ dns_domain }}/fullchain.pem"
BLOG_HTTPSKEY="/etc/letsencrypt/live/{{ dns_domain }}/privkey.pem"
BLOG_IMAGECACHE=true
